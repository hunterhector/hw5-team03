swer xmi:id="830" sofa="12" begin="16689" end="16700" text="APP/Ps mice" id="3" isCorrect="true" isSelected="false" nerList=""/><types:Answer xmi:id="845" sofa="12" begin="16704" end="16709" text="mouse" id="4" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="893" sofa="12" begin="16779" end="16788" text="fractions" id="0" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="908" sofa="12" begin="16792" end="16809" text="monomeric amyloid" id="1" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="923" sofa="12" begin="16813" end="16822" text="membranes" id="2" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="938" sofa="12" begin="16826" end="16844" text="synaptic terminals" id="3" isCorrect="true" isSelected="false" nerList=""/><types:Answer xmi:id="953" sofa="12" begin="16848" end="16853" text="lipid" id="4" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="1001" sofa="12" begin="16912" end="16928" text="transgenic mouse" id="0" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="1016" sofa="12" begin="16932" end="16970" text="predominantly in synaptic mitochondria" id="1" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="1031" sofa="12" begin="16974" end="16988" text="choroid plexus" id="2" isCorrect="false" isSelected="false" nerList=""/><types:Answer xmi:id="1046" sofa="12" begin="16992" end="17024" text="before amyloid-beta accumulation" id="3" isCorrect="true" isSelected="false" nerList="3443"/><types:Answer xmi:id="1061" sofa="12" begin="17028" end="17038" text="postmortem" id="4" isCorrect="false" isSelected="false" nerList=""/><types:QuestionAnswerSet xmi:id="120" sofa="12" begin="0" end="0" question="19" answerList="29 44 59 74 89"/><types:QuestionAnswerSet xmi:id="228" sofa="12" begin="0" end="0" question="127" answerList="137 152 167 182 197"/><types:QuestionAnswerSet xmi:id="336" sofa="12" begin="0" end="0" quest